    "htmlhint": "^1.1.4",
    "lite-server": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-standard": "^33.0.0"
  },
  "dependencies": {
